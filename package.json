{
  "name": "react-typescript-todoapp",
  "version": "1.0.0",
  "description": "A simple todo app built with React and TypeScript",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-typescript-todoapp.git"
  },
  "author": "born3am",
  "homepage": "https://todoapp-react-typescript.vercel.app",
  "type": "module",
  "prettier": "@born3am/prettier-config",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint -c eslint.config.js --cache .",
    "lint:fix": "npm run lint -- --no-cache --fix",
    "prettier": "prettier --check --cache --no-error-on-unmatched-pattern '**/*.md' '**/*.yml' '**/*.json' '**/*.html' '**/*.css' '**/*.scss' '**/*.sass' '**/*.less'",
    "prettier:fix": "npm run prettier -- --no-cache --write",
    "fix": "npm run prettier:fix ; npm run lint:fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "animate.css": "^4.1.1",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-dom": "^18.3.1",
    "sweetalert2": "11.15.10",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@born3am/eslint-config": "^0.3.3",
    "@born3am/prettier-config": "^1.0.1",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^18.3.1",
    "@types/sweetalert": "^2.0.4",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.5.4",
    "vite": "^6.0.2"
  }
}
